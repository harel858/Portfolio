{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node-12"
    }
  ],
  "headers": [
    {
      "source": "*",
      "headers": ["Content-Type", "Access-Control-Allow-Headers"]
    }
  ]
}
